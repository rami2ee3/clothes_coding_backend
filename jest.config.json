{
  "collectCoverage": true,
  "modulePaths": ["<rootDir>"],
  "moduleFileExtensions": ["js", "mjs"],
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.mjs$": "babel-jest"
  },
  "testRegex": "((\\.|/*.)(test))\\.js?$",
  "setupFiles": ["<rootDir>/test/setup/dotenv"],
  "transformIgnorePatterns": ["node_modules/(?!@camelcase-keys)/"]
}